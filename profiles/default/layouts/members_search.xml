 <property name="flexible_widgets"/>
 <property name="validate_values_expr"></property>
 <widget name="email" meta_type="String Widget">
  <property name="fields">
   <element value="email"/>
  </property>
  <property name="label">label_email</property>
  <property name="label_edit">label_email</property>
  <property name="description">Member email</property>
  <property name="is_i18n">True</property>
  <property name="display_width">30</property>
 </widget>
 <widget name="givenName" meta_type="String Widget">
  <property name="fields">
   <element value="givenName"/>
  </property>
  <property name="label">label_first_name</property>
  <property name="label_edit">label_first_name</property>
  <property name="description">Member first name</property>
  <property name="is_i18n">True</property>
 </widget>
 <widget name="id" meta_type="String Widget">
  <property name="fields">
   <element value="id"/>
  </property>
  <property name="label">label_user_name</property>
  <property name="label_edit">label_user_name</property>
  <property name="description">Member login</property>
  <property name="is_i18n">True</property>
  <property name="display_width">30</property>
 </widget>
 <widget name="sn" meta_type="String Widget">
  <property name="fields">
   <element value="sn"/>
  </property>
  <property name="label">label_last_name</property>
  <property name="label_edit">label_last_name</property>
  <property name="description">Member last name</property>
  <property name="is_i18n">True</property>
 </widget>
 <table>
  <row>
   <cell name="id" ncols="2"/>
  </row>
  <row>
   <cell name="givenName"/>
