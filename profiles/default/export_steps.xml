<?xml version="1.0"?>
<export-steps>
 <export-step id="actions"
              handler="Products.CMFSetup.actions.exportActionProviders"
              title="Action Providers">
  
  
  
  
  Export actions tool's action providers and their actions.
 
 
 
 
 </export-step>
 <export-step id="directories"
              handler="Products.CPSDirectory.exportimport.exportDirectoryTool"
              title="Directory Tool">
  Export directory tool's directory configuration.
 </export-step>
 <export-step id="layouts"
              handler="Products.CPSSchemas.setup.layout.exportLayoutTool"
              title="Layout Tool">
  
  
  
  
  Export layout tool's layouts and widgets.
 
 
 
 
 </export-step>
 <export-step id="properties"
              handler="Products.CMFSetup.properties.exportSiteProperties"
              title="Site Properties">
  
  
  
  
  Export site properties.
 
 
 
 
 </export-step>
 <export-step id="rolemap"
              handler="Products.CMFSetup.rolemap.exportRolemap"
              title="Role / Permission Map">
  
  
  
  
  Export custom roles and non-default role-permission mappings.
 
 
 
 
 </export-step>
 <export-step id="schemas"
              handler="Products.CPSSchemas.setup.schema.exportSchemaTool"
              title="Schema Tool">
  
  
  
  
  Export schema tool's schemas and fields.
 
 
 
 
 </export-step>
 <export-step id="skins"
              handler="Products.CMFSetup.skins.exportSkinsTool"
              title="Skins Tool">
  
  
  
  
  Export skins tool's filesystem directory views and skin path definitions.
 
 
 
 
 </export-step>
 <export-step id="step_registries"
              handler="Products.CMFSetup.tool.exportStepRegistries"
              title="Step Registries">
  
  
  
  
  Export current contents of import step registry and export step registry.
 
 
 
 
 </export-step>
 <export-step id="toolset"
              handler="Products.CMFSetup.tool.exportToolset"
              title="Required Tools">
  
  
  
  
  Export required / forbidden tools.
 
 
 
 
 </export-step>
 <export-step id="typeinfo"
              handler="Products.CPSDocument.setup.handlers.exportTypesTool"
              title="Types Tool">
  
  
  
  
  Export types tool's type information objects.
 
 
 
 
 </export-step>
 <export-step id="userfolder"
              handler="Products.CPSUserFolder.setup.handlers.exportUserFolder"
              title="User Folder">
  
  
  
  
  Export user folder configuration.
 
 
 
 
 </export-step>
 <export-step id="vocabularies"
              handler="Products.CPSSchemas.exportimport.vocabulary.exportVocabularyTool"
              title="Vocabulary Tool">
  Export vocabulary tool and vocabularies.
 </export-step>
