<tal:block define="
  file_name python:request['file'];
  file_html_name python:request['file_html'];
  ">

  <metal:block use-macro="here/content_lib/macros/master">

    <metal:block fill-slot="base">
      <base href="" tal:attributes="href python:'%s/preview?file=%s&amp;file_html=%s' % (here_url, file_name, file_html_name)" />
    </metal:block>

    <metal:block fill-slot="main">
      <tal:block tal:content="structure python:getattr(doc, file_html_name)" />
      <!--
      <tal:block tal:content="structure python:request['URL']" />
      -->
    </metal:block>

  </metal:block>

</tal:block>


