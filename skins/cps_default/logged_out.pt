<metal:html tal:define="showthirdcol nothing">

<metal:body use-macro="here/main_template/macros/master">

<metal:main fill-slot="main">

  <p tal:condition="isAnon"
     i18n:translate="description_logout_success">
    You have been logged out.
  </p>

  <p tal:condition="not:isAnon">
    <span i18n:translate="description_logout_zmi_warning">
      You are logged in outside the portal.  You may need to
    </span>
    <a href="/manage_zmi_logout"
       i18n:translate="link_logout_zmi">
      log out of the Zope management interface
    </a>.
  </p>

</metal:main>
</metal:body>
</metal:html>
