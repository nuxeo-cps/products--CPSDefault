<dtml-var manage_page_header>

<h3> Add Box Slot </h3>

<p> A Slot is a logical position for boxes.</p>

<form action="addBoxSlot" method="POST">
<table border="0">
  <tr>
    <td class="form-label">Id</td>
    <td class="form-label"><input type="text" name="id"></td>
  </tr>
  <tr>
    <td class="form-optional">Title</td>
    <td class="form-label"><input type="text" name="title"></td>
  </tr>
</table>
<input type="submit" name="submit" value=" Create ">
</form>
<dtml-var manage_page_footer>
