<?xml version="1.0"?>
<object name="cpsdefault_search" meta_type="CPS Layout">
 <property name="layout_create_method"></property>
 <property name="layout_edit_method"></property>
 <property name="layout_view_method"></property>
 <property name="style_prefix">layout_default_</property>
 <property name="flexible_widgets"/>
 <property name="validate_values_expr"></property>
 <widget name="Language" meta_type="Search Language Widget">
  <property name="fields">
   <element value="Language"/>
  </property>
  <property name="label_edit">label_language</property>
  <property name="is_i18n">True</property>
 </widget>
 <widget name="Subject" meta_type="MultiSelect Widget">
  <property name="fields">
   <element value="Subject"/>
  </property>
  <property name="label_edit">label_subject</property>
  <property name="is_i18n">True</property>
  <property name="vocabulary">subject_voc</property>
  <property name="translated">True</property>
  <property name="size">4</property>
 </widget>
 <widget name="ZCText" meta_type="Search ZCText Widget">
  <property name="fields">
   <element value="SearchableText"/>
   <element value="Title"/>
   <element value="Description"/>
  </property>
  <property name="label_edit">label_searchadv_full_text</property>
  <property name="is_i18n">True</property>
 </widget>
 <widget name="modified" meta_type="Search Modified Widget">
  <property name="fields">
   <element value="modified"/>
   <element value="modified_usage"/>
  </property>
  <property name="label_edit">label_searchadv_updated</property>
  <property name="is_i18n">True</property>
 </widget>
 <widget name="path" meta_type="Search Location Widget">
  <property name="fields">
   <element value="path"/>
  </property>
  <property name="label_edit">label_search_location</property>
  <property name="is_i18n">True</property>
 </widget>
 <widget name="portal_type" meta_type="MultiSelect Widget">
  <property name="fields">
   <element value="portal_type"/>
  </property>
  <property name="label_edit">label_search_portal_type</property>
  <property name="is_i18n">True</property>
  <property name="vocabulary">search_portal_type</property>
  <property name="size">4</property>
 </widget>
 <widget name="review_state" meta_type="Select Widget">
  <property name="fields">
   <element value="review_state"/>
  </property>
  <property name="label_edit">label_search_status</property>
  <property name="is_i18n">True</property>
  <property name="vocabulary">search_review_state</property>
 </widget>
 <widget name="sort_by" meta_type="Search Sort Widget">
  <property name="fields">
   <element value="sort-on"/>
   <element value="sort-order"/>
   <element value="sort-limit"/>
  </property>
  <property name="label_edit">label_searchadv_sort_by</property>
  <property name="is_i18n">True</property>
 </widget>
 <table>
  <row>
   <cell name="ZCText" ncols="2"/>
  </row>
  <row>
   <cell name="Subject"/>
   <cell name="portal_type"/>
  </row>
  <row>
   <cell name="modified"/>
   <cell name="review_state"/>
  </row>
  <row>
   <cell name="path" ncols="2"/>
  </row>
  <row>
   <cell name="Language" ncols="2"/>
  </row>
  <row>
   <cell name="sort_by" ncols="2"/>
  </row>
 </table>
</object>
