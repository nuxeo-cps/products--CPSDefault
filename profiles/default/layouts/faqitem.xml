<?xml version="1.0"?>
<layout>
  <widget id="Description" kind="CPS Text Widget">
    <property name="css_class">ddescription</property>
    <property name="fields">
     <element value="Description" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsdoc_faq_long_question_label_edit</property>
    <property name="width">72</property>
  </widget>
  <widget id="Title" kind="CPS Heading Widget">
    <property name="fields">
     <element value="Title" /></property>
    <property name="is_i18n">1</property>
    <property name="is_required">1</property>
    <property name="label_edit">cpsdoc_faq_question_label_edit</property>
    <property name="level">2</property>
    <property name="size_max">200</property>
  </widget>
  <widget id="content" kind="CPS Text Widget">
    <property name="css_class">dcontent</property>
    <property name="description">FAQ answer</property>
    <property name="fields">
     <element value="content" />
     <element value="content_position" />
     <element value="content_format" /></property>
    <property name="is_i18n">1</property>
    <property name="is_required">1</property>
    <property name="label">cpsdoc_faq_answer_label</property>
    <property name="label_edit">cpsdoc_faq_answer_label_edit</property>
    <property name="width">72</property>
  </widget>
  <table>
    <row>
      <cell id="Title"/>
    </row>
    <row>
      <cell id="Description"/>
    </row>
    <row>
      <cell id="content"/>
    </row>
  </table>
  <property name="style_prefix">layout_default_</property>
  <property name="layout_create_method"></property>
  <property name="layout_edit_method"></property>
  <property name="layout_view_method"></property>
  <property name="flexible_widgets"></property>
  <property name="validate_values_expr"></property>
</layout>
