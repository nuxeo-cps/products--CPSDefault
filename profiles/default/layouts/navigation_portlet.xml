<?xml version="1.0"?>
<layout>
  <widget id="authorized_only" type="Boolean Widget">
    <property name="fields">
     <element value="authorized_only" /></property>
    <property name="hidden_if_expr">python: context.display != 'extended_site_map'</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_authorized_only_label</property>
  </widget>
  <widget id="context_is_portlet" type="CheckBox Widget">
    <property name="fields">
     <element value="context_is_portlet" /></property>
    <property name="hidden_if_expr">python: context.display not in ['folder_contents', 'folder_contents_dropdown_list']</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="hidden_readonly_layout_modes"></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_nav_context_is_portlet_label</property>
    <property name="readonly_layout_modes"></property>
  </widget>
  <widget id="context_rpath" type="String Widget">
    <property name="display_width">30</property>
    <property name="fields">
     <element value="context_rpath" /></property>
    <property name="hidden_if_expr">python: context.display not in ['folder_contents', 'folder_contents_dropdown_list'] or context.context_is_portlet</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="hidden_readonly_layout_modes"></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_nav_context_rpath_label</property>
    <property name="readonly_layout_modes"></property>
  </widget>
  <widget id="contextual" type="Boolean Widget">
    <property name="fields">
     <element value="contextual" /></property>
    <property name="hidden_if_expr">python: context.display in ['vertical_breadcrumbs_menu', 'folder_contents', 'subfolder_contents', 'site_map', 'collapsible_menu', 'up_to_parent', 'folder_contents_dropdown_list']</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="hidden_readonly_layout_modes"></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_contextual_label</property>
    <property name="readonly_layout_modes"></property>
  </widget>
  <widget id="display" type="Select Widget">
    <property name="fields">
     <element value="display" /></property>
    <property name="help">cpsportlets_display_mode_help</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_display_mode_label</property>
    <property name="translated">True</property>
    <property name="vocabulary">cpsportlets_navigation_display_voc</property>
  </widget>
  <widget id="display_description" type="Boolean Widget">
    <property name="fields">
     <element value="display_description" /></property>
    <property name="hidden_if_expr">python: context.display not in ['extended_site_map', 'site_map', 'folder_contents', 'subfolder_contents', 'navigation_tree', 'collapsible_menu']</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_display_description_label</property>
  </widget>
  <widget id="display_hidden_folders" type="Boolean Widget">
    <property name="fields">
     <element value="display_hidden_folders" /></property>
    <property name="hidden_if_expr">python: context.display in ['up_to_parent']</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_display_hidden_folders_label</property>
  </widget>
  <widget id="display_managers" type="Boolean Widget">
    <property name="fields">
     <element value="display_managers" /></property>
    <property name="hidden_if_expr">python: context.display != 'extended_site_map'</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_display_managers_label</property>
  </widget>
  <widget id="end_depth" type="Int Widget">
    <property name="fields">
     <element value="end_depth" /></property>
    <property name="hidden_if_expr">python: context.display in ['folder_contents', 'subfolder_contents', 'collapsible_menu', 'up_to_parent', 'folder_contents_dropdown_list']</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="hidden_readonly_layout_modes"></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_end_depth_label</property>
    <property name="readonly_layout_modes"></property>
  </widget>
  <widget id="folder_prefixes" type="Lines Widget">
    <property name="fields">
     <element value="folder_prefixes" /></property>
    <property name="hidden_if_expr">python: context.display != 'site_map'</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="hidden_readonly_layout_modes"></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_folder_prefixes_label</property>
    <property name="readonly_layout_modes"></property>
  </widget>
  <widget id="portlet" type="Generic Portlet Widget">
    <property name="fields">
     <element value="portlet" /></property>
    <property name="hidden_layout_modes">
     <element value="edit" /></property>
    <property name="render_method">widget_portlet_navigation</property>
  </widget>
  <widget id="rel_depth" type="Int Widget">
    <property name="fields">
     <element value="rel_depth" /></property>
    <property name="hidden_if_expr">python: context.display in ['folder_contents', 'subfolder_contents', 'navigation_tree', 'site_map', 'up_to_parent', 'folder_contents_dropdown_list']</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="hidden_readonly_layout_modes"></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_rel_depth_label</property>
    <property name="readonly_layout_modes"></property>
  </widget>
  <widget id="root_uids" type="Lines Widget">
    <property name="fields">
     <element value="root_uids" /></property>
    <property name="hidden_if_expr">python: context.display in ['vertical_breadcrumbs_menu', 'folder_contents', 'up_to_parent', 'folder_contents_dropdown_list']</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_root_uids_label</property>
  </widget>
  <widget id="show_docs" type="Boolean Widget">
    <property name="fields">
     <element value="show_docs" /></property>
    <property name="hidden_if_expr">python: context.display not in ['folder_contents', 'collapsible_menu', 'folder_contents_dropdown_list']</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="hidden_readonly_layout_modes"></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_show_docs_label</property>
    <property name="readonly_layout_modes"></property>
  </widget>
  <widget id="show_icons" type="Boolean Widget">
    <property name="fields">
     <element value="show_icons" /></property>
    <property name="hidden_if_expr">python: context.display in ['vertical_breadcrumbs_menu', 'folder_contents', 'subfolder_contents', 'up_to_parent', 'folder_contents_dropdown_list']</property>
    <property name="hidden_layout_modes">
     <element value="view" /></property>
    <property name="hidden_readonly_layout_modes"></property>
    <property name="is_i18n">True</property>
    <property name="label_edit">cpsportlets_navigation_show_icons_label</property>
    <property name="readonly_layout_modes"></property>
  </widget>
  <table>
    <row>
      <cell id="portlet" ncols="6"/>
    </row>
    <row>
      <cell id="display" ncols="6"/>
    </row>
    <row>
      <cell id="root_uids"/>
      <cell id="rel_depth"/>
      <cell id="end_depth"/>
      <cell id="contextual"/>
      <cell id="context_is_portlet"/>
      <cell id="context_rpath"/>
    </row>
    <row>
      <cell id="folder_prefixes" ncols="6"/>
    </row>
    <row>
      <cell id="show_docs"/>
      <cell id="show_icons"/>
      <cell id="display_hidden_folders" ncols="4"/>
    </row>
    <row>
      <cell id="display_managers"/>
      <cell id="display_description"/>
      <cell id="authorized_only" ncols="4"/>
    </row>
  </table>
  <property name="layout_create_method"></property>
  <property name="layout_edit_method"></property>
  <property name="layout_view_method"></property>
  <property name="style_prefix">layout_portlet_</property>
  <property name="flexible_widgets"></property>
  <property name="validate_values_expr"></property>
</layout>
