<?xml version="1.0"?>
<type-info id="Event" kind="Factory-based Type Information">
  <property name="title">portal_type_Calendar_Event_title</property>
  <property name="description">portal_type_Calendar_Event_description</property>
  <property name="content_icon">event_icon.png</property>
  <property name="content_meta_type">Event</property>
  <property name="product">CPSCalendar</property>
  <property name="factory">addEvent</property>
  <property name="immediate_view">event_edit_form</property>
  <property name="global_allow">True</property>
  <property name="filter_content_types">0</property>
  <property name="allowed_content_types">[]</property>
  <property name="allow_discussion">0</property>
  <property name="cps_is_searchable">0</property>
  <property name="cps_proxy_type"></property>
  <property name="cps_display_as_document_in_listing">0</property>
  <property name="cps_is_portalbox">0</property>
  <aliases>
   <alias from="(Default)" to="calendar_event_view"/>
   <alias from="view" to="calendar_event_view"/>
  </aliases>
  <action action_id="view" title="action_view"
          condition_expr=""
          url_expr="string:${object_url}/calendar_event_view"
          category="object" visible="True">
   <permission>View</permission>
  </action>
  <action action_id="edit" title="action_modify"
          condition_expr="python:object.getCalendar() == object.getOrganizerCalendar()"
          url_expr="string:${object_url}/calendar_editevent_form"
          category="object" visible="True">
   <permission>Modify portal content</permission>
  </action>
  <action action_id="attendees" title="action_attendees"
          condition_expr="python:object.getCalendar() == object.getOrganizerCalendar()"
          url_expr="string:${object_url}/calendar_attendees_form"
          category="object" visible="True">
   <permission>Modify portal content</permission>
  </action>
  <action action_id="delete" title="action_delete"
          condition_expr="python:object.getCalendar() == object.getOrganizerCalendar() and object.event_status != &quot;canceled&quot;"
          url_expr="string:${object_url}/calendar_delevent"
          category="object" visible="True">
   <permission>Modify portal content</permission>
  </action>
  <action action_id="calendar" title="action_calendar"
          condition_expr=""
          url_expr="string:${object_url}/../"
          category="object" visible="True">
   <permission>View</permission>
  </action>
  <action action_id="create" title="action_create"
          condition_expr=""
          url_expr="string:${object_url}/calendar_eventcreate_form"
          category="object" visible="False">
  </action>
</type-info>
