<?xml version="1.0"?>
<layout>
  <widget id="batch_size" kind="CPS Int Widget">
    <property name="fields">
     <element value="batch_size" /></property>
    <property name="is_i18n">1</property>
    <property name="is_limited">1</property>
    <property name="label_edit">cpsnav_option_batch_size</property>
    <property name="max_value">100</property>
    <property name="min_value">1</property>
  </widget>
  <widget id="current_uid" kind="CPS String Widget">
    <property name="display_width">72</property>
    <property name="fields">
     <element value="current_uid" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_option_current_uid</property>
    <property name="size_max">255</property>
  </widget>
  <widget id="display_mode" kind="CPS Select Widget">
    <property name="fields">
     <element value="display_mode" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_option_display_mode</property>
    <property name="vocabulary">navigation_display_mode</property>
  </widget>
  <widget id="expand_all" kind="CPS Boolean Widget">
    <property name="fields">
     <element value="expand_all" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_search_expand_all</property>
  </widget>
  <widget id="filter_listing_ptypes"
          kind="CPS MultiSelect Widget">
    <property name="fields">
     <element value="filter_listing_ptypes" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_option_filter_listing_ptypes</property>
    <property name="size">10</property>
    <property name="vocabulary">navigation_filter_listing_ptypes</property>
  </widget>
  <widget id="filter_tree_ptypes"
          kind="CPS MultiSelect Widget">
    <property name="fields">
     <element value="filter_tree_ptypes" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_option_filter_tree_ptypes</property>
    <property name="size">10</property>
    <property name="vocabulary">navigation_filter_tree_ptypes</property>
  </widget>
  <widget id="include_root" kind="CPS Boolean Widget">
    <property name="fields">
     <element value="include_root" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_option_include_root</property>
  </widget>
  <widget id="no_nodes" kind="CPS Boolean Widget">
    <property name="fields">
     <element value="no_nodes" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_option_no_node</property>
  </widget>
  <widget id="sort_listing_by" kind="CPS Select Widget">
    <property name="fields">
     <element value="sort_listing_by" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_option_sort_listing_by</property>
    <property name="vocabulary">navigation_sort_listing_by</property>
  </widget>
  <widget id="sort_listing_direction"
          kind="CPS Select Widget">
    <property name="fields">
     <element value="sort_listing_direction" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_option_sort_listing_direction</property>
    <property name="vocabulary">navigation_sort_direction</property>
  </widget>
  <widget id="sort_tree_by" kind="CPS Select Widget">
    <property name="fields">
     <element value="sort_tree_by" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_option_sort_tree_by</property>
    <property name="vocabulary">navigation_sort_tree_by</property>
  </widget>
  <widget id="sort_tree_direction" kind="CPS Select Widget">
    <property name="fields">
     <element value="sort_tree_direction" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">cpsnav_option_sort_tree_direction</property>
    <property name="vocabulary">navigation_sort_direction</property>
  </widget>
  <table>
    <row>
      <cell id="current_uid" ncols="2"/>
    </row>
    <row>
      <cell id="display_mode" ncols="2"/>
    </row>
    <row>
      <cell id="include_root" ncols="2"/>
    </row>
    <row>
      <cell id="sort_tree_by"/>
      <cell id="sort_tree_direction"/>
    </row>
    <row>
      <cell id="expand_all" ncols="2"/>
    </row>
    <row>
      <cell id="sort_listing_by"/>
      <cell id="sort_listing_direction"/>
    </row>
    <row>
      <cell id="batch_size" ncols="2"/>
    </row>
    <row>
      <cell id="no_nodes" ncols="2"/>
    </row>
    <row>
      <cell id="filter_tree_ptypes"/>
      <cell id="filter_listing_ptypes"/>
    </row>
  </table>
  <property name="style_prefix">layout_default_</property>
  <property name="layout_create_method"></property>
  <property name="layout_edit_method"></property>
  <property name="layout_view_method"></property>
  <property name="flexible_widgets"></property>
  <property name="validate_values_expr"></property>
</layout>
