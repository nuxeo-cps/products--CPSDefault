     expression="o.isCPSFolderish()"/>
  <filtered_set name="searchable" meta_type="PythonFilteredSet"
     expression="not filter(lambda s: s.startswith('portal_') or s and s[0] in ('.', '_'), o.getPhysicalPath())"/>
