<?xml version="1.0"?>
<layout>
  <widget id="allow_anon_posts" kind="CPS Boolean Widget">
    <property name="fields">
     <element value="allow_anon_posts" /></property>
    <property name="is_i18n">1</property>
    <property name="label">forum_allow_anon_posts</property>
    <property name="label_edit">forum_allow_anon_posts</property>
  </widget>
  <widget id="frozen_forum" kind="CPS Boolean Widget">
    <property name="fields">
     <element value="frozen_forum" /></property>
    <property name="is_i18n">1</property>
    <property name="label">forum_label_lock_forum</property>
    <property name="label_edit">forum_label_lock_forum</property>
  </widget>
  <widget id="moderation_mode" kind="CPS Boolean Widget">
    <property name="fields">
     <element value="moderation_mode" /></property>
    <property name="help">forum_help_moderation</property>
    <property name="is_i18n">1</property>
    <property name="label">forum_moderation</property>
    <property name="label_edit">forum_moderation</property>
    <property name="label_false">forum_a_posteriori</property>
    <property name="label_true">forum_a_priori</property>
  </widget>
  <widget id="threads_per_page" kind="CPS Int Widget">
    <property name="fields">
     <element value="threads_per_page" /></property>
    <property name="is_i18n">1</property>
    <property name="label">forum_label_threads_per_page</property>
    <property name="label_edit">forum_label_threads_per_page</property>
  </widget>
  <widget id="tree_display" kind="CPS Select Widget">
    <property name="fields">
     <element value="tree_display" /></property>
    <property name="is_i18n">1</property>
    <property name="label">forum_label_tree_display</property>
    <property name="label_edit">forum_label_tree_display</property>
    <property name="translated">1</property>
    <property name="vocabulary">forum_tree_voc</property>
  </widget>
  <widget id="wf_display" kind="CPS Select Widget">
    <property name="fields">
     <element value="wf_display" /></property>
    <property name="is_i18n">1</property>
    <property name="label_edit">forum_label_wf_display</property>
    <property name="translated">1</property>
    <property name="vocabulary">forum_wf_display_voc</property>
  </widget>
  <table>
    <row>
      <cell id="moderation_mode"/>
    </row>
    <row>
      <cell id="allow_anon_posts"/>
    </row>
    <row>
      <cell id="frozen_forum"/>
    </row>
    <row>
      <cell id="tree_display"/>
    </row>
    <row>
      <cell id="wf_display"/>
    </row>
    <row>
      <cell id="threads_per_page"/>
    </row>
  </table>
  <property name="style_prefix">layout_default_</property>
  <property name="layout_create_method"></property>
  <property name="layout_edit_method"></property>
  <property name="layout_view_method"></property>
  <property name="flexible_widgets"></property>
  <property name="validate_values_expr"></property>
</layout>
