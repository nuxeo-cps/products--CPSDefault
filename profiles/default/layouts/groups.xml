<?xml version="1.0"?>
<layout>
  <widget id="group" kind="CPS Identifier Widget">
    <property name="fields">
     <element value="group" /></property>
    <property name="is_i18n">1</property>
    <property name="is_required">1</property>
    <property name="label">label_group</property>
    <property name="label_edit">label_group</property>
    <property name="readonly_layout_modes">
     <element value="edit" /></property>
  </widget>
  <widget id="members" kind="CPS MultiSelect Widget">
    <property name="fields">
     <element value="members" /></property>
    <property name="is_i18n">1</property>
    <property name="label">label_members</property>
    <property name="label_edit">label_members</property>
    <property name="size">7</property>
    <property name="vocabulary">members</property>
  </widget>
  <widget id="subgroups" kind="CPS MultiSelect Widget">
    <property name="fields">
     <element value="subgroups" /></property>
    <property name="hidden_if_expr">not:context/hasSubGroupsSupport</property>
    <property name="hidden_layout_modes">
     <element value="search" /></property>
    <property name="hidden_readonly_layout_modes">
     <element value="edit" />
     <element value="create" />
     <element value="view" /></property>
    <property name="is_i18n">1</property>
    <property name="label">label_subgroups</property>
    <property name="label_edit">label_subgroups</property>
    <property name="size">7</property>
    <property name="vocabulary">groups</property>
  </widget>
  <table>
    <row>
      <cell id="group"/>
    </row>
    <row>
      <cell id="members"/>
    </row>
    <row>
      <cell id="subgroups"/>
    </row>
  </table>
  <property name="style_prefix">layout_dir_</property>
  <property name="layout_create_method"></property>
  <property name="layout_edit_method"></property>
  <property name="layout_view_method"></property>
  <property name="flexible_widgets"></property>
  <property name="validate_values_expr"></property>
</layout>
