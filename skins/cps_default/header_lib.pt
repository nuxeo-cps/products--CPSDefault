<!-- macro library for html head overrides -->
<!-- $Id$ -->

<!-- ============================================================ -->
<metal:block define-macro="header">
  <meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
  <title tal:content="here/expanded_title">Nuxeo | Collaborative Portal
    Server</title>
  <metal:block define-slot="base"></metal:block>
  <link rel="Stylesheet" type="text/css" href=""
    tal:attributes="href string:${base_url}ploneNS4.css">
  <style type="text/css" media="all"
    tal:content="string:@import url(${base_url}plone.css);"></style>
  <style type="text/css" media="all"
    tal:condition="exists: here/ploneCustom.css"
    tal:content="string:@import url(${base_url}ploneCustom.css);"></style>
  <link rel="stylesheet" type="text/css" media="print" href=""
    tal:attributes="href string:${base_url}plonePrint.css">
  <link rel="icon" href="" type="image/png" tal:attributes="href
    string:${base_url}favicon.png">
  <meta http-equiv="imagetoolbar" content="no">
  <metal:block define-slot="head_slot"></metal:block>
  <metal:block define-slot="css_slot"></metal:block>
  <metal:block define-slot="javascript_head_slot"></metal:block>
</metal:block>
